{
  "name": "zmarks",
  "title": "zMarks",
  "description": "Search and view zMarks",
  "subtitle": "zMarks",
  "icon": "command-icon.png",
  "author": "mrmartineau",
  "license": "MIT",
  "commands": [
    {
      "name": "search",
      "title": "Search zMarks",
      "subtitle": "zMarks",
      "description": "Search zMarks",
      "mode": "view"
    },
    {
      "name": "recent",
      "title": "Recently added zMarks",
      "subtitle": "zMarks",
      "description": "View recently added zMarks",
      "mode": "view"
    },
    {
      "name": "add",
      "title": "Add new bookmark",
      "subtitle": "zMarks",
      "description": "Add new bookmark",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "zMarksBasePath",
      "type": "textfield",
      "title": "zMarks API base path",
      "description": "e.g. https://zmarks.zander.wtf",
      "required": true,
      "default": "",
      "value": ""
    },
    {
      "name": "zMarksApiSecret",
      "type": "password",
      "title": "zMarks API secret",
      "description": "This is the Supabase `service_role` API key",
      "required": true,
      "default": "",
      "value": ""
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.26.3",
    "node-fetch": "^3.0.0",
    "parse-github-url": "^1.0.2",
    "proper-url-join": "^2.1.1"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/parse-github-url": "^1.0.0",
    "@types/proper-url-join": "^2.1.1",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop"
  }
}
